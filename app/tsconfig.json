{
  "compilerOptions": {
    "target": "es6",
    "composite": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "esModuleInterop": true,
    "module": "es2015",
    "moduleResolution": "Node",
    "isolatedModules": true,
    "jsx": "react-jsx",
    "incremental": true,
    "outDir": "dist",
    "paths": {
      "@mui/material/*": [
        "./node_modules/@apicize/toolkit/*",
        "./node_modules/@mui/material/*"
      ],
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
  ],
  "exclude": [
    "bdd.d.ts",
    "node_modules",
    "src-tauri"
  ]
}